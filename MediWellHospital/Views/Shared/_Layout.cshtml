@using Business.ViewModels;
@model HomeVM

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Hospital Management System</title>
    <link rel="stylesheet" href="./assets/css/style.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css" integrity="sha384-zCbKRCUGaJDkqS1kPbPd7TveP5iyJE0EjAuZQTgFLD2ylzuqKfdKlfG/eSrtxUkn" crossorigin="anonymous">

    <link rel="stylesheet" href="./assets/owl-carousel/owl-carousel files/owl.carousel.min.css">
    <link rel="stylesheet" href="./assets/owl-carousel/owl-carousel files/owl.theme.default.min.css">
    <link href="~/assets/lib/jquery-ui-1.13.1.custom/jquery-ui.css" rel="stylesheet" />
</head>
<body>

    <!-- ======= Top Bar ======= -->
    <section id="topbar" class="d-flex align-items-center">
        <div class="container d-flex justify-content-center justify-content-md-between">
            <div class="contact-info d-flex align-items-center">
                <i class="fas fa-envelope d-flex align-items-center"><a href="mailto:contact@example.com">@Model.Setting["hospitalEmail"]</a></i>
                <i class="fas fa-mobile d-flex align-items-center ms-4"><span>@Model.Setting["hospitalPhone"]</span></i>
            </div>
            <div class="social-links d-none d-md-flex align-items-center">
                <a href="#" class="twitter"><i class="fab fa-twitter"></i></a>
                <a href="#" class="facebook"><i class="fab fa-facebook"></i></a>
                <a href="#" class="instagram"><i class="fab fa-instagram"></i></a>
                <a href="#" class="youtube"><i class="fab fa-youtube"></i></i></a>
            </div>
        </div>
    </section>
    <!-- ======= Top Bar ======= -->
    <!--Navbar Section Start-->

    <nav id="header" class="navbar navbar-expand-lg navbar-light">
        <div class="container">
            <a class="header-logo" href="#">
                <img src="./assets/images/@Model.Setting["navbarLogoImage"]" alt="">
            </a>

            <div class="nav-section">
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>


                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active">
                            <a class="nav-link scrollto hover-underline-animation" asp-controller="Home" asp-action="Index" >Ana səhifə </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link scrollto hover-underline-animation" href="#Departaments">Departamentlər</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link scrollto hover-underline-animation" href="#our-testimonial">Şərhlər</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link scrollto hover-underline-animation" href="#Doctors">Həkimlər</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link scrollto hover-underline-animation" asp-controller="Home" asp-action="Contact"  >Əlaqə</a>
                        </li>
                    </ul>
                </div>

                <a asp-controller="Home" asp-action="Contact" class="appointment-btn scrollto"><span class="d-none d-md-inline">Görüş təyin</span> et</a>
            </div>
        </div>

    </nav>

    <!--Navbar Section End-->
    @RenderBody()
    <!-- ======= Footer ======= -->
    <footer id="Footer">
        <div class="footer-top">
            <div class="container">
                <div class="row">

                    <div class="col-lg-3 col-md-6">
                        <div class="footer-info">
                            <h3>Medicio</h3>
                            <p>
                                A108 Adam Street <br>
                                NY 535022, USA<br><br>
                                <strong>Phone:</strong> +1 5589 55488 55<br>
                                <strong>Email:</strong> contact@example.com<br>
                            </p>
                            <div class="social-links mt-3 d-none d-md-flex align-items-center">
                                <a href="#" class="twitter"><i class="fab fa-twitter"></i></a>
                                <a href="#" class="facebook"><i class="fab fa-facebook"></i></a>
                                <a href="#" class="instagram"><i class="fab fa-instagram"></i></a>
                                <a href="#" class="youtube"><i class="fab fa-youtube"></i></i></a>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-9 col-md-6">
